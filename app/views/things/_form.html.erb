<%= form_for @thing do |t| %>

	<%= t.text_field :name, placeholder: "Name" %> <br>
	<%= t.text_field :category, placeholder: "Category" %> <br>
	<%= t.text_area  :why, placeholder: "Why do you love it?" %> <br>
	<%= t.text_field :street, placeholder: "Street Address" %> <br>
	<%= t.text_field :city, placeholder: "City" %> <br>
	<%= t.text_field :state, placeholder: "State" %> <br>
	<%= t.text_field :zip_code, placeholder: "Zip" %> <br>
	<%= t.submit %> <br>

<% end %>

<%= link_to "Cancel", profile_path(current_user.id)%>